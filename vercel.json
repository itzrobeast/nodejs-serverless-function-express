{
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "instagram-webhook.js", "use": "@vercel/node" },
    { "src": "google-calendar.js", "use": "@vercel/node" },
    { "src": "assistant.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/instagram-webhook", "dest": "instagram-webhook.js" },
    { "src": "/google-calendar", "dest": "google-calendar.js" },
    { "src": "/assistant", "dest": "assistant.js" },
    { "src": "/setup-business", "dest": "/setup-business.js" },
    { "src": "/(.*)", "dest": "index.js" }
  ]
}
