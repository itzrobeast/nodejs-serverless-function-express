{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "setup-business.js", "use": "@vercel/node" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/setup-business", "dest": "setup-business.js" },
    { "src": "/setup-business/health", "dest": "setup-business.js" },
    { "src": "/get-business", "dest": "index.js" },
    { "src": "/assistant", "dest": "index.js" },
    { "src": "/instagram-webhook", "dest": "index.js" },
    { "src": "/refresh-token", "dest": "index.js" },
    { "src": "/verify-session", "dest": "index.js" },
    { "src": "/get-vonage-number", "dest": "index.js" },
    { "src": "/auth", "dest": "index.js" },
    { "src": "/retrieve-leads", "dest": "index.js" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
    { "src": "/favicon.png", "dest": "/public/favicon.png" },
    { "src": "/(.*)", "dest": "index.js" }
  ],
  "functions": {
    "index.js": { "runtime": "nodejs16.x" },
    "setup-business.js": { "runtime": "nodejs16.x" }
  }
}
