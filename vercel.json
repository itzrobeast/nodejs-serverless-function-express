{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "instagram-webhook.js", "use": "@vercel/node" },
    { "src": "google-calendar.js", "use": "@vercel/node" },
    { "src": "assistant.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/setup-business", "dest": "index.js" },
    { "src": "/instagram-webhook", "dest": "instagram-webhook.js" },
    { "src": "/google-calendar", "dest": "google-calendar.js" },
    { "src": "/assistant", "dest": "assistant.js" },
    { "src": "/favicon.ico", "dest": "favicon.ico" },
    { "src": "/favicon.png", "dest": "favicon.png" },
    { "src": "/(.*)", "dest": "index.js" }
  ],
  "headers": [
    {
      "source": "/setup-business",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "https://mila-verse.vercel.app" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" },
        { "key": "Access-Control-Allow-Credentials", "value": "true" }
      ]
    }
  ]
}
